CREATE TABLE GENBLASTER.ORF (ID BIGINT PRIMARY KEY NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1), GENOME_NAME VARCHAR(50)NOT NULL, FRAME SMALLINT NOT NULL, AC INTEGER NOT NULL, AASEQ LONG VARCHAR NOT NULL, NUSEQ LONG VARCHAR NOT NULL)
CREATE TABLE GENBLASTER.BLAST (ID BIGINT PRIMARY KEY NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),QUERY_GENOME_NAME VARCHAR(50) NOT NULL, TARGET_GENOME_NAME VARCHAR(50) NOT NULL, QSEQID VARCHAR(50) NOT NULL, SSEQID VARCHAR(50) NOT NULL, EVALUE DOUBLE PRECISION NOT NULL, SLEN SMALLINT NOT NULL, QSTART SMALLINT NOT NULL, QEND SMALLINT NOT NULL, SSTART SMALLINT NOT NULL, SEND SMALLINT NOT NULL, PIDENT DOUBLE PRECISION NOT NULL, NIDENT DOUBLE PRECISION NOT NULL, MISMATCH DOUBLE PRECISION NOT NULL, POSITIVE DOUBLE PRECISION NOT NULL, GAPOPEN SMALLINT NOT NULL, GAPS SMALLINT NOT NULL)